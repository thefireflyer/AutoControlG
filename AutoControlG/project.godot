; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="AutoControlG"
run/main_scene="res://src/scenes/Main.tscn"
boot_splash/image="res://assets/splash.png"
boot_splash/fullsize=false
boot_splash/bg_color=Color( 0.12549, 0.145098, 0.192157, 1 )
config/icon="res://assets/logo.png"
config/windows_native_icon="res://assets/logo.ico"

[autoload]

ClassLibrary="*res://src/scripts/ClassLibrary.gd"
FunctionLibrary="*res://src/scripts/FunctionLibrary.gd"
DataLibrary="*res://src/scripts/DataLibrary.gd"

[display]

window/size/width=1290
window/size/height=800
window/size/borderless=true
window/size/always_on_top=true
window/per_pixel_transparency/allowed=true
window/per_pixel_transparency/enabled=true
window/stretch/mode="2d"
window/stretch/aspect="keep"

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_clear_color=Color( 0.113725, 0.113725, 0.113725, 0 )
environment/default_environment="res://default_env.tres"
